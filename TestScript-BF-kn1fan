LevelFarm:AddButton({
    Name = "–û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Ñ–∞—Ä–º 2 –º–æ—Ä–µ",
    Description = "–û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ—Ç —Ñ–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π –Ω–∞ 2 –º–æ—Ä–µ",
    Callback = function()
        stopLevelFarm()
    end
})

