                    mobPositions = {Vector3.new(150, 50, 150), Vector3.new(50, 50, 50)}
                }
            elseif level < 20 then
                return {
                    npc = "Jungle Quest Giver",
                    npcPosition = Vector3.new(-1500, 50, 100),
                    mobName = "Monkey",
