local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()
workspace.FallenPartsDestroyHeight = -math.huge

local Window = Library:MakeWindow({
    Title = "lava Hub | Blox Fruits ",
    SubTitle = "by: @kn1fan / lava85halst",
    LoadText = "Loading lava Hub",
    Flags = "ChaosHub_Broookhaven"
})
Window:AddMinimizeButton({
    Button = { Image = "rbxassetid://72674068736695", BackgroundTransparency = 0 },
    Corner = { CornerRadius = UDim.new(35, 1) },
})

-- ... (–ø—Ä–µ–¥—ã–¥—É—â–∏–µ –≤–∫–ª–∞–¥–∫–∏ Info –∏ –§–∞—Ä–º —Å—É–Ω–¥—É–∫–æ–≤ –æ—Å—Ç–∞—é—Ç—Å—è –±–µ–∑ –∏–∑–º–µ–Ω–µ–Ω–∏–π)

local LevelFarm = Window:MakeTab({ Title = "–§–∞—Ä–º lvl", Icon = "rbxassetid://13364900349" })

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Ñ–∞—Ä–º–∏–Ω–≥–æ–º —É—Ä–æ–≤–Ω–µ–π
local isLevelFarming = false
local currentSea = 1
local noClipConnection
local levelFarmThread

-- –°–µ–∫—Ü–∏—è –¥–ª—è 1 –º–æ—Ä—è
LevelFarm:AddSection({ "1 –ú–æ—Ä–µ" })

LevelFarm:AddButton({
    Name = "–í–∫–ª—é—á–∏—Ç—å —Ñ–∞—Ä–º 1 –º–æ—Ä–µ",
    Description = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Ñ–∞—Ä–º–∏—Ç —É—Ä–æ–≤–Ω–∏ –Ω–∞ 1 –º–æ—Ä–µ",
    Callback = function()
        if isLevelFarming then
            print("–§–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π —É–∂–µ –∑–∞–ø—É—â–µ–Ω")
            return
        end
        
        currentSea = 1
        startLevelFarm()
    end
})

-- –°–µ–∫—Ü–∏—è –¥–ª—è 2 –º–æ—Ä—è
LevelFarm:AddSection({ "2 –ú–æ—Ä–µ" })

LevelFarm:AddButton({
    Name = "–í–∫–ª—é—á–∏—Ç—å —Ñ–∞—Ä–º 2 –º–æ—Ä–µ",
    Description = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Ñ–∞—Ä–º–∏—Ç —É—Ä–æ–≤–Ω–∏ –Ω–∞ 2 –º–æ—Ä–µ",
    Callback = function()
        if isLevelFarming then
            print("–§–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π —É–∂–µ –∑–∞–ø—É—â–µ–Ω")
            return
        end
        
        currentSea = 2
        startLevelFarm()
    end
})

-- –°–µ–∫—Ü–∏—è –¥–ª—è 3 –º–æ—Ä—è
LevelFarm:AddSection({ "3 –ú–æ—Ä–µ" })

LevelFarm:AddButton({
    Name = "–í–∫–ª—é—á–∏—Ç—å —Ñ–∞—Ä–º 3 –º–æ—Ä–µ",
    Description = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Ñ–∞—Ä–º–∏—Ç —É—Ä–æ–≤–Ω–∏ –Ω–∞ 3 –º–æ—Ä–µ",
    Callback = function()
        if isLevelFarming then
            print("–§–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π —É–∂–µ –∑–∞–ø—É—â–µ–Ω")
            return
        end
        
        currentSea = 3
        startLevelFarm()
    end
})

-- –ö–Ω–æ–ø–∫–∞ –æ—Å—Ç–∞–Ω–æ–≤–∫–∏
LevelFarm:AddButton({
    Name = "–û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Ñ–∞—Ä–º —É—Ä–æ–≤–Ω–µ–π",
    Description = "–û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ—Ç —Ñ–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π",
    Callback = function()
        stopLevelFarm()
    end
})

-- –§—É–Ω–∫—Ü–∏—è –æ—Å—Ç–∞–Ω–æ–≤–∫–∏ —Ñ–∞—Ä–º–∞ —É—Ä–æ–≤–Ω–µ–π
function stopLevelFarm()
    if isLevelFarming then
        isLevelFarming = false
        
        -- –û—Ç–∫–ª—é—á–∞–µ–º NoClip
        if noClipConnection then
            noClipConnection:Disconnect()
            noClipConnection = nil
        end
        
        -- –û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø–æ—Ç–æ–∫ —Ñ–∞—Ä–º–∞
        if levelFarmThread then
            coroutine.close(levelFarmThread)
            levelFarmThread = nil
        end
        
        -- –û—Ç–∫–ª—é—á–∞–µ–º –ø–æ–ª–µ—Ç
        local Players = game:GetService("Players")
        local localPlayer = Players.LocalPlayer
        local character = localPlayer.Character
        
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.PlatformStand = false
            end
            
            local bodyVelocity = character.HumanoidRootPart:FindFirstChild("BodyVelocity")
            if bodyVelocity then
                bodyVelocity:Destroy()
            end
            
            -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
        
        print("‚úÖ –§–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
    else
        print("‚ùå –§–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π –Ω–µ –±—ã–ª –∑–∞–ø—É—â–µ–Ω")
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –±–µ–∑–æ–ø–∞—Å–Ω–æ–π –ø—Ä–æ–≤–µ—Ä–∫–∏ –ø–æ–∑–∏—Ü–∏–∏
local function isPositionSafe(position)
    return position.Y > 10 and position.Y < 1000
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –±–µ–∑–æ–ø–∞—Å–Ω–æ–π –ø–æ–∑–∏—Ü–∏–∏
local function getSafePosition(position)
    if isPositionSafe(position) then
        return position
    else
        -- –ï—Å–ª–∏ –ø–æ–∑–∏—Ü–∏—è –Ω–µ–±–µ–∑–æ–ø–∞—Å–Ω–∞, –≤–æ–∑–≤—Ä–∞—â–∞–µ–º –ø–æ–∑–∏—Ü–∏—é –Ω–∞–¥ –∑–µ–º–ª–µ–π
        return Vector3.new(position.X, 50, position.Z)
    end
end

-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Ñ–∞—Ä–º–∞ —É—Ä–æ–≤–Ω–µ–π
function startLevelFarm()
    local Players = game:GetService("Players")
    local localPlayer = Players.LocalPlayer
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    
    isLevelFarming = true
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è NoClip
    local function enableNoClip()
        local character = localPlayer.Character
        if not character then return end
        
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide then
                part.CanCollide = false
            end
        end
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è –ø–æ–ª–µ—Ç–∞
    local function enableFlight()
        local character = localPlayer.Character
        if not character then return false end
        
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if not humanoid then return false end
        
        humanoid.PlatformStand = true
        
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.MaxForce = Vector3.new(100000, 100000, 100000)
        bodyVelocity.Parent = character.HumanoidRootPart
        
        return true
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–∫–ª—é—á–µ–Ω–∏—è –ø–æ–ª–µ—Ç–∞
    local function disableFlight()
        local character = localPlayer.Character
        if not character then return end
        
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.PlatformStand = false
        end
        
        local bodyVelocity = character.HumanoidRootPart:FindFirstChild("BodyVelocity")
        if bodyVelocity then
            bodyVelocity:Destroy()
        end
    end
    
    -- –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª–µ—Ç–∞ –∫ —Ç–æ—á–∫–µ —Å –∑–∞—â–∏—Ç–æ–π –æ—Ç –ø–æ–¥–∑–µ–º–Ω—ã—Ö –ø–æ–∑–∏—Ü–∏–π
    local function flyToPosition(targetPosition, speed)
        local character = localPlayer.Character
        if not character or not character:FindFirstChild("HumanoidRootPart") then
            return false
        end
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –∏ –∫–æ—Ä—Ä–µ–∫—Ç–∏—Ä—É–µ–º —Ü–µ–ª–µ–≤—É—é –ø–æ–∑–∏—Ü–∏—é
        local safeTargetPosition = getSafePosition(targetPosition)
        if safeTargetPosition ~= targetPosition then
            print("‚ö†Ô∏è –ö–æ—Ä—Ä–µ–∫—Ç–∏—Ä—É–µ–º –Ω–µ–±–µ–∑–æ–ø–∞—Å–Ω—É—é –ø–æ–∑–∏—Ü–∏—é: " .. tostring(targetPosition) .. " -> " .. tostring(safeTargetPosition))
            targetPosition = safeTargetPosition
        end
        
        local rootPart = character.HumanoidRootPart
        local bodyVelocity = rootPart:FindFirstChild("BodyVelocity")
        
        if not bodyVelocity then
            enableFlight()
            bodyVelocity = rootPart:FindFirstChild("BodyVelocity")
            if not bodyVelocity then return false end
        end
        
        local startTime = tick()
        local maxTime = 10
        
        while isLevelFarming and character and rootPart do
            local distance = (targetPosition - rootPart.Position).Magnitude
            
            if distance < 10 then
                break
            end
            
            if tick() - startTime > maxTime then
                print("‚è∞ –¢–∞–π–º–∞—É—Ç –ø–æ–ª–µ—Ç–∞")
                break
            end
            
            enableNoClip()
            
            local direction = (targetPosition - rootPart.Position).Unit
            local velocity = direction * math.min(speed, distance * 2)
            
            bodyVelocity.Velocity = velocity
            wait(0.03)
        end
        
        if bodyVelocity then
            bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        end
        
        return true
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Ç–µ–∫—É—â–µ–≥–æ —É—Ä–æ–≤–Ω—è –∏–≥—Ä–æ–∫–∞
    local function getPlayerLevel()
        return localPlayer.Data.Level.Value
    end
    
    -- –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ –∫–≤–µ—Å—Ç–µ —Å –±–µ–∑–æ–ø–∞—Å–Ω—ã–º–∏ –ø–æ–∑–∏—Ü–∏—è–º–∏
    local function getQuestInfo(level)
        if currentSea == 1 then
            -- 1 –º–æ—Ä–µ - —É—Ä–æ–≤–Ω–∏ 1-700 (–±–µ–∑–æ–ø–∞—Å–Ω—ã–µ –ø–æ–∑–∏—Ü–∏–∏ —Å Y > 20)
            if level < 10 then
                return {
                    npc = "Start Island Quest Giver",
                    npcPosition = Vector3.new(100, 50, 100),
                    mobName = "Bandit",
                    mobPositions = {Vector3.new(150, 50, 150), Vector3.new(50, 50, 50)}
                }
            elseif level < 20 then
                return {
                    npc = "Monkey Quest Giver",
                    npcPosition = Vector3.new(-1500, 50, 100),
                    mobName = "Monkey",
                    mobPositions = {Vector3.new(-1550, 50, 150), Vector3.new(-1450, 50, 50)}
                }
            elseif level < 30 then
                return {
                    npc = "Gorilla Quest Giver",
                    npcPosition = Vector3.new(-1200, 50, -450),
                    mobName = "Gorilla",
                    mobPositions = {Vector3.new(-1250, 50, -400), Vector3.new(-1150, 50, -500)}
                }
            elseif level < 40 then
                return {
                    npc = "Pirate Quest Giver",
                    npcPosition = Vector3.new(-1100, 50, 3800),
                    mobName = "Pirate",
                    mobPositions = {Vector3.new(-1150, 50, 3850), Vector3.new(-1050, 50, 3750)}
                }
            elseif level < 60 then
                return {
                    npc = "Brute Quest Giver",
                    npcPosition = Vector3.new(1100, 50, -4700),
                    mobName = "Brute",
                    mobPositions = {Vector3.new(1050, 50, -4650), Vector3.new(1150, 50, -4750)}
                }
            elseif level < 75 then
                return {
                    npc = "Desert Bandit Quest Giver",
                    npcPosition = Vector3.new(900, 50, 3700),
                    mobName = "Desert Bandit",
                    mobPositions = {Vector3.new(850, 50, 3750), Vector3.new(950, 50, 3650)}
                }
            elseif level < 90 then
                return {
                    npc = "Snow Bandit Quest Giver",
                    npcPosition = Vector3.new(1100, 150, -4700),
                    mobName = "Snow Bandit",
                    mobPositions = {Vector3.new(1050, 150, -4650), Vector3.new(1150, 150, -4750)}
                }
            elseif level < 120 then
                return {
                    npc = "Marine Quest Giver",
                    npcPosition = Vector3.new(-4800, 50, 4300),
                    mobName = "Marine",
                    mobPositions = {Vector3.new(-4850, 50, 4350), Vector3.new(-4750, 50, 4250)}
                }
            elseif level < 150 then
                return {
                    npc = "Chief Petty Officer Quest Giver",
                    npcPosition = Vector3.new(-4800, 50, 4300),
                    mobName = "Chief Petty Officer",
                    mobPositions = {Vector3.new(-4850, 50, 4350), Vector3.new(-4750, 50, 4250)}
                }
            elseif level < 190 then
                return {
                    npc = "Sky Bandit Quest Giver",
                    npcPosition = Vector3.new(-4600, 700, -2600),
                    mobName = "Sky Bandit",
                    mobPositions = {Vector3.new(-4650, 700, -2550), Vector3.new(-4550, 700, -2650)}
                }
            elseif level < 240 then
                return {
                    npc = "Dark Master Quest Giver",
                    npcPosition = Vector3.new(-3800, 50, -3500),
                    mobName = "Dark Master",
                    mobPositions = {Vector3.new(-3850, 50, -3450), Vector3.new(-3750, 50, -3550)}
                }
            elseif level < 300 then
                return {
                    npc = "Prisoner Quest Giver",
                    npcPosition = Vector3.new(4800, 50, -2200),
                    mobName = "Prisoner",
                    mobPositions = {Vector3.new(4750, 50, -2150), Vector3.new(4850, 50, -2250)}
                }
            else
                return {
                    npc = "Diamond Quest Giver",
                    npcPosition = Vector3.new(-1500, 50, 1000),
                    mobName = "Diamond",
                    mobPositions = {Vector3.new(-1550, 50, 1050), Vector3.new(-1450, 50, 950)}
                }
            end
        
        elseif currentSea == 2 then
            -- 2 –º–æ—Ä–µ - —É—Ä–æ–≤–Ω–∏ 700-1500
            if level < 750 then
                return {
                    npc = "Marine Captain Quest Giver",
                    npcPosition = Vector3.new(1400, 80, 100),
                    mobName = "Marine Captain",
                    mobPositions = {Vector3.new(1350, 80, 150), Vector3.new(1450, 80, 50)}
                }
            elseif level < 800 then
                return {
                    npc = "Galley Captain Quest Giver",
                    npcPosition = Vector3.new(-500, 80, 1200),
                    mobName = "Galley Captain",
                    mobPositions = {Vector3.new(-550, 80, 1250), Vector3.new(-450, 80, 1150)}
                }
            elseif level < 850 then
                return {
                    npc = "Forest Pirate Quest Giver",
                    npcPosition = Vector3.new(-1500, 80, 100),
                    mobName = "Forest Pirate",
                    mobPositions = {Vector3.new(-1550, 80, 150), Vector3.new(-1450, 80, 50)}
                }
            else
                return {
                    npc = "High Level 2 Quest Giver",
                    npcPosition = Vector3.new(-1200, 80, 400),
                    mobName = "Galley Captain",
                    mobPositions = {Vector3.new(-1250, 80, 450), Vector3.new(-1150, 80, 350)}
                }
            end
        
        elseif currentSea == 3 then
            -- 3 –º–æ—Ä–µ - —É—Ä–æ–≤–Ω–∏ 1500+
            if level < 1550 then
                return {
                    npc = "Pirate Millionaire Quest Giver",
                    npcPosition = Vector3.new(-5000, 80, -5000),
                    mobName = "Pirate Millionaire",
                    mobPositions = {Vector3.new(-5050, 80, -4950), Vector3.new(-4950, 80, -5050)}
                }
            elseif level < 1600 then
                return {
                    npc = "Dragon Crew Archer Quest Giver",
                    npcPosition = Vector3.new(-3000, 80, -6000),
                    mobName = "Dragon Crew Archer",
                    mobPositions = {Vector3.new(-3050, 80, -5950), Vector3.new(-2950, 80, -6050)}
                }
            else
                return {
                    npc = "Dragon Crew Warrior Quest Giver",
                    npcPosition = Vector3.new(-1000, 80, -7000),
                    mobName = "Dragon Crew Warrior",
                    mobPositions = {Vector3.new(-1050, 80, -6950), Vector3.new(-950, 80, -7050)}
                }
            end
        end
        
        -- –ï—Å–ª–∏ –Ω–∏—á–µ–≥–æ –Ω–µ –Ω–∞–π–¥–µ–Ω–æ, –≤–æ–∑–≤—Ä–∞—â–∞–µ–º –±–∞–∑–æ–≤—ã–π –∫–≤–µ—Å—Ç —Å –±–µ–∑–æ–ø–∞—Å–Ω–æ–π –ø–æ–∑–∏—Ü–∏–µ–π
        return {
            npc = "Default Quest Giver",
            npcPosition = Vector3.new(0, 50, 0),
            mobName = "Bandit",
            mobPositions = {Vector3.new(50, 50, 50), Vector3.new(-50, 50, -50)}
        }
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ NPC –ø–æ –∏–º–µ–Ω–∏
    local function findNPC(npcName)
        -- –ò—â–µ–º –≤ —Ä–∞–∑–Ω—ã—Ö –≤–æ–∑–º–æ–∂–Ω—ã—Ö –º–µ—Å—Ç–∞—Ö
        local locations = {
            workspace.NPCs,
            workspace.Live,
            workspace
        }
        
        for _, location in ipairs(locations) do
            for _, npc in pairs(location:GetChildren()) do
                if npc.Name:lower():find(npcName:lower()) and npc:FindFirstChild("HumanoidRootPart") then
                    return npc
                end
            end
        end
        
        return nil
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–∑—è—Ç–∏—è –∫–≤–µ—Å—Ç–∞
    local function takeQuest(questInfo)
        local character = localPlayer.Character
        if not character then return false end
        
        print("üõ´ –õ–µ—Ç–∏–º –∫ NPC: " .. questInfo.npc)
        
        -- –°–Ω–∞—á–∞–ª–∞ –ø—ã—Ç–∞–µ–º—Å—è –Ω–∞–π—Ç–∏ NPC –ø–æ –∏–º–µ–Ω–∏
        local npc = findNPC(questInfo.npc)
        local npcPosition = getSafePosition(questInfo.npcPosition)  -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –±–µ–∑–æ–ø–∞—Å–Ω—É—é –ø–æ–∑–∏—Ü–∏—é
        
        if npc then
            npcPosition = getSafePosition(npc.HumanoidRootPart.Position)
            print("‚úÖ –ù–∞—à–ª–∏ NPC –≤ –∏–≥—Ä–µ: " .. npc.Name)
        else
            print("‚ö†Ô∏è NPC –Ω–µ –Ω–∞–π–¥–µ–Ω, –∏—Å–ø–æ–ª—å–∑—É–µ–º –∑–∞–ø–∞—Å–Ω—É—é –ø–æ–∑–∏—Ü–∏—é")
        end
        
        -- –õ–µ—Ç–∏–º –∫ NPC
        local flySuccess = flyToPosition(npcPosition, 150)
        
        if flySuccess then
            wait(1)
            
            -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ NPC, –ø–æ–¥–ª–µ—Ç–∞–µ–º –±–ª–∏–∂–µ
            if npc then
                local npcSafePosition = getSafePosition(npc.HumanoidRootPart.Position + Vector3.new(0, 5, 0))
                flyToPosition(npcSafePosition, 50)
                wait(0.5)
                
                -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º –¥–∏–∞–ª–æ–≥ —Å NPC
                local args = {
                    [1] = npc.Name,
                    [2] = "StartQuest"
                }
                
                -- –ü—ã—Ç–∞–µ–º—Å—è –≤–∑—è—Ç—å –∫–≤–µ—Å—Ç —á–µ—Ä–µ–∑ RemoteEvent
                pcall(function()
                    ReplicatedStorage.Remotes.CommF_:InvokeServer(unpack(args))
                end)
                
                wait(1)
                return true
            end
        else
            print("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –¥–æ–ª–µ—Ç–µ—Ç—å –¥–æ NPC")
        end
        
        return false
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –º–æ–±–æ–≤
    local function findMobs(mobName)
        local mobs = {}
        
        -- –ò—â–µ–º –≤ —Ä–∞–∑–Ω—ã—Ö –≤–æ–∑–º–æ–∂–Ω—ã—Ö –º–µ—Å—Ç–∞—Ö
        local locations = {
            workspace.Enemies,
            workspace.Live,
            workspace.Mobs,
            workspace
        }
        
        for _, location in ipairs(locations) do
            for _, mob in pairs(location:GetChildren()) do
                if mob:FindFirstChild("Humanoid") and mob.Humanoid.Health > 0 then
                    if mob.Name:lower():find(mobName:lower()) then
                        table.insert(mobs, mob)
                    end
                end
            end
        end
        
        return mobs
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞—Ç–∞–∫–∏ –º–æ–±–æ–≤
    local function attackMobs(questInfo)
        local character = localPlayer.Character
        if not character then return false end
        
        print("üîç –ò—â–µ–º –º–æ–±–æ–≤: " .. questInfo.mobName)
        
        -- –õ–µ—Ç–∏–º –∫ –∑–æ–Ω–µ —Å–ø–∞–≤–Ω–∞ –º–æ–±–æ–≤ (–∏—Å–ø–æ–ª—å–∑—É–µ–º –±–µ–∑–æ–ø–∞—Å–Ω—É—é –ø–æ–∑–∏—Ü–∏—é)
        local safeMobPosition = getSafePosition(questInfo.mobPositions[1])
        flyToPosition(safeMobPosition, 120)
        wait(0.5)
        
        local mobsKilled = 0
        local maxMobs = 10 -- –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –º–æ–±–æ–≤ –¥–ª—è –∫–≤–µ—Å—Ç–∞
        
        while isLevelFarming and mobsKilled < maxMobs do
            local character = localPlayer.Character
            if not character then break end
            
            -- –ò—â–µ–º –º–æ–±–æ–≤
            local mobs = findMobs(questInfo.mobName)
            
            if #mobs > 0 then
                -- –°–æ—Ä—Ç–∏—Ä—É–µ–º –ø–æ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—é
                table.sort(mobs, function(a, b)
                    local aDist = (a.HumanoidRootPart.Position - character.HumanoidRootPart.Position).Magnitude
                    local bDist = (b.HumanoidRootPart.Position - character.HumanoidRootPart.Position).Magnitude
                    return aDist < bDist
                end)
                
                local targetMob = mobs[1]
                print("‚öîÔ∏è –ê—Ç–∞–∫—É–µ–º –º–æ–±–∞: " .. targetMob.Name)
                
                -- –õ–µ—Ç–∏–º –∫ –º–æ–±—É –Ω–∞ –±–µ–∑–æ–ø–∞—Å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ
                local attackPosition = getSafePosition(targetMob.HumanoidRootPart.Position + Vector3.new(10, 5, 10))
                flyToPosition(attackPosition, 80)
                
                -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —Ñ—Ä—É–∫—Ç –¥–ª—è –∞—Ç–∞–∫–∏
                useFruitAttack(targetMob)
                
                -- –ñ–¥–µ–º —Å–º–µ—Ä—Ç–∏ –º–æ–±–∞
                local mobAlive = true
                local attackTime = tick()
                
                while mobAlive and isLevelFarming and (tick() - attackTime < 10) do
                    if not targetMob or not targetMob.Parent or targetMob.Humanoid.Health <= 0 then
                        mobAlive = false
                        mobsKilled = mobsKilled + 1
                        print("‚úÖ –ú–æ–± —É–±–∏—Ç! –í—Å–µ–≥–æ: " .. mobsKilled .. "/" .. maxMobs)
                        break
                    end
                    
                    -- –ü—Ä–æ–¥–æ–ª–∂–∞–µ–º –∞—Ç–∞–∫–æ–≤–∞—Ç—å
                    useFruitAttack(targetMob)
                    wait(0.5)
                end
                
            else
                -- –ï—Å–ª–∏ –º–æ–±–æ–≤ –Ω–µ—Ç, –ø–∞—Ç—Ä—É–ª–∏—Ä—É–µ–º –∑–æ–Ω—É
                print("üîç –ú–æ–±–æ–≤ –Ω–µ –Ω–∞–π–¥–µ–Ω–æ, –ø–∞—Ç—Ä—É–ª–∏—Ä—É–µ–º –∑–æ–Ω—É...")
                for _, pos in pairs(questInfo.mobPositions) do
                    if not isLevelFarming then break end
                    flyToPosition(getSafePosition(pos), 100)
                    wait(1)
                end
            end
            
            wait(0.5)
        end
        
        return mobsKilled >= maxMobs
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –∞—Ç–∞–∫–∏ —Ñ—Ä—É–∫—Ç–∞
    local function useFruitAttack(target)
        local character = localPlayer.Character
        if not character or not target then return end
        
        -- –í–∫–ª—é—á–∞–µ–º NoClip –¥–ª—è –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏
        enableNoClip()
        
        -- –ò—â–µ–º —Ñ—Ä—É–∫—Ç –≤ –∏–Ω–≤–µ–Ω—Ç–∞—Ä–µ
        local backpack = localPlayer.Backpack
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        
        if not humanoid then return end
        
        -- –ü—ã—Ç–∞–µ–º—Å—è –Ω–∞–π—Ç–∏ —Ñ—Ä—É–∫—Ç
        for _, tool in pairs(backpack:GetChildren()) do
            if tool:IsA("Tool") and (tool.Name:find("Fruit") or tool.Name:find("Blox")) then
                -- –ë–µ—Ä–µ–º –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç –≤ —Ä—É–∫–∏
                humanoid:EquipTool(tool)
                wait(0.2)
                
                -- –ò—Å–ø–æ–ª—å–∑—É–µ–º Z –∞—Ç–∞–∫—É (–æ—Å–Ω–æ–≤–Ω–∞—è –∞—Ç–∞–∫–∞)
                local args = {
                    [1] = "Z",
                    [2] = target.HumanoidRootPart.Position
                }
                
                pcall(function()
                    ReplicatedStorage.Remotes.CommF_:InvokeServer(unpack(args))
                end)
                
                -- –ò—Å–ø–æ–ª—å–∑—É–µ–º X –∞—Ç–∞–∫—É (–≤—Ç–æ—Ä–∞—è –∞—Ç–∞–∫–∞)
                local args2 = {
                    [1] = "X",
                    [2] = target.HumanoidRootPart.Position
                }
                
                pcall(function()
                    ReplicatedStorage.Remotes.CommF_:InvokeServer(unpack(args2))
                end)
                
                break
            end
        end
        
        -- –ë–∞–∑–æ–≤–∞—è –∞—Ç–∞–∫–∞ –º–µ—á–æ–º (–µ—Å–ª–∏ —Ñ—Ä—É–∫—Ç–∞ –Ω–µ—Ç)
        for _, tool in pairs(backpack:GetChildren()) do
            if tool:IsA("Tool") and tool.Name:find("Sword") then
                humanoid:EquipTool(tool)
                wait(0.2)
                
                -- –ê—Ç–∞–∫–∞ –º–µ—á–æ–º
                mouse1click()
                wait(0.1)
                break
            end
        end
    end
    
    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–¥–∞—á–∏ –∫–≤–µ—Å—Ç–∞
    local function completeQuest(questInfo)
        local character = localPlayer.Character
        if not character then return false end
        
        print("üì§ –°–¥–∞–µ–º –∫–≤–µ—Å—Ç...")
        
        -- –°–Ω–æ–≤–∞ –∏—â–µ–º NPC
        local npc = findNPC(questInfo.npc)
        local npcPosition = getSafePosition(questInfo.npcPosition)
        
        if npc then
            npcPosition = getSafePosition(npc.HumanoidRootPart.Position)
        end
        
        -- –õ–µ—Ç–∏–º –æ–±—Ä–∞—Ç–Ω–æ –∫ NPC
        local flySuccess = flyToPosition(npcPosition, 150)
        
        if flySuccess then
            wait(1)
            
            -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ NPC, –ø–æ–¥–ª–µ—Ç–∞–µ–º –±–ª–∏–∂–µ
            if npc then
                local npcSafePosition = getSafePosition(npc.HumanoidRootPart.Position + Vector3.new(0, 5, 0))
                flyToPosition(npcSafePosition, 50)
                wait(0.5)
                
                -- –°–¥–∞–µ–º –∫–≤–µ—Å—Ç
                local args = {
                    [1] = npc.Name,
                    [2] = "EndQuest"
                }
                
                pcall(function()
                    ReplicatedStorage.Remotes.CommF_:InvokeServer(unpack(args))
                end)
                
                wait(1)
                return true
            end
        else
            print("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –¥–æ–ª–µ—Ç–µ—Ç—å –¥–æ NPC –¥–ª—è —Å–¥–∞—á–∏ –∫–≤–µ—Å—Ç–∞")
        end
        
        return false
    end
    
    -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ NoClip
    noClipConnection = game:GetService("RunService").Stepped:Connect(function()
        if isLevelFarming and localPlayer.Character then
            enableNoClip()
        end
    end)
    
    -- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª —Ñ–∞—Ä–º–∞ —É—Ä–æ–≤–Ω–µ–π
    levelFarmThread = coroutine.create(function()
        print("üöÄ –ó–∞–ø—É—Å–∫–∞–µ–º —Ñ–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π –Ω–∞ " .. currentSea .. " –º–æ—Ä–µ...")
        
        while isLevelFarming do
            local character = localPlayer.Character
            if not character then
                character = localPlayer.CharacterAdded:Wait()
                wait(2)
            end
            
            if not character or not character:FindFirstChild("HumanoidRootPart") then
                wait(1)
                coroutine.yield()
            end
            
            -- –í–∫–ª—é—á–∞–µ–º –ø–æ–ª–µ—Ç
            enableFlight()
            enableNoClip()
            
            local playerLevel = getPlayerLevel()
            print("üìä –¢–µ–∫—É—â–∏–π —É—Ä–æ–≤–µ–Ω—å: " .. playerLevel)
            
            -- –ü–æ–ª—É—á–∞–µ–º –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –∫–≤–µ—Å—Ç–µ –¥–ª—è —Ç–µ–∫—É—â–µ–≥–æ —É—Ä–æ–≤–Ω—è
            local questInfo = getQuestInfo(playerLevel)
            
            if questInfo then
                print("üéØ –ù–∞—á–∏–Ω–∞–µ–º –∫–≤–µ—Å—Ç: " .. questInfo.npc .. " (–ú–æ–±—ã: " .. questInfo.mobName .. ")")
                
                -- –ë–µ—Ä–µ–º –∫–≤–µ—Å—Ç
                if takeQuest(questInfo) then
                    print("‚úÖ –ö–≤–µ—Å—Ç –≤–∑—è—Ç, –Ω–∞—á–∏–Ω–∞–µ–º —Ñ–∞—Ä–º –º–æ–±–æ–≤...")
                    
                    -- –§–∞—Ä–º–∏–º –º–æ–±–æ–≤
                    if attackMobs(questInfo) then
                        print("üéâ –í—Å–µ –º–æ–±—ã —É–±–∏—Ç—ã, —Å–¥–∞–µ–º –∫–≤–µ—Å—Ç...")
                        
                        -- –°–¥–∞–µ–º –∫–≤–µ—Å—Ç
                        if completeQuest(questInfo) then
                            print("üèÜ –ö–≤–µ—Å—Ç —É—Å–ø–µ—à–Ω–æ —Å–¥–∞–Ω!")
                        else
                            print("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å —Å–¥–∞—Ç—å –∫–≤–µ—Å—Ç")
                        end
                    else
                        print("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å —É–±–∏—Ç—å –≤—Å–µ—Ö –º–æ–±–æ–≤")
                    end
                else
                    print("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –≤–∑—è—Ç—å –∫–≤–µ—Å—Ç")
                end
            else
                print("‚ùå –ù–µ –Ω–∞–π–¥–µ–Ω –ø–æ–¥—Ö–æ–¥—è—â–∏–π –∫–≤–µ—Å—Ç –¥–ª—è —É—Ä–æ–≤–Ω—è " .. playerLevel)
            end
            
            wait(2)
            coroutine.yield()
        end
        
        -- –û—á–∏—Å—Ç–∫–∞ –ø—Ä–∏ –æ—Å—Ç–∞–Ω–æ–≤–∫–µ
        if noClipConnection then
            noClipConnection:Disconnect()
            noClipConnection = nil
        end
        disableFlight()
        
        print("üõë –§–∞—Ä–º–∏–Ω–≥ —É—Ä–æ–≤–Ω–µ–π –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
    end)
    
    -- –ó–∞–ø—É—Å–∫–∞–µ–º –ø–æ—Ç–æ–∫
    coroutine.resume(levelFarmThread)
end

LevelFarm:AddSection({ "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è" })
LevelFarm:AddParagraph({"–ò—Å–ø—Ä–∞–≤–ª–µ–Ω–∏—è:", "‚Ä¢ –ó–∞—â–∏—Ç–∞ –æ—Ç —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –ø–æ–¥ –∫–∞—Ä—Ç—É ‚Ä¢ –ë–µ–∑–æ–ø–∞—Å–Ω—ã–µ –ø–æ–∑–∏—Ü–∏–∏ ‚Ä¢ –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –æ—Ç–∫–ª—é—á–µ–Ω–∏—è"})
LevelFarm:AddParagraph({"–§—É–Ω–∫—Ü–∏–∏:", "‚Ä¢ –ê–≤—Ç–æ–ø–æ–ª–µ—Ç –∫ NPC ‚Ä¢ –ê–≤—Ç–æ–∞—Ç–∞–∫–∞ —Ñ—Ä—É–∫—Ç–æ–º ‚Ä¢ –ê–≤—Ç–æ—Å–¥–∞—á–∞ –∫–≤–µ—Å—Ç–∞ ‚Ä¢ –õ–µ–≥–∫–æ–µ –æ—Ç–∫–ª—é—á–µ–Ω–∏–µ"})
LevelFarm:AddParagraph({"–ë–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç—å:", "‚Ä¢ –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–æ–∑–∏—Ü–∏–π ‚Ä¢ –ö–æ—Ä—Ä–µ–∫—Ç–∏—Ä–æ–≤–∫–∞ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç ‚Ä¢ NoClip –∑–∞—â–∏—Ç–∞"})
